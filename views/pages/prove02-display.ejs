  <div class="container">
    <h1><%= title %></h1>
  </div>
</div>

<p>Thanks for your imput!</p>
<h3>Your books:</h3><br>
<% if (bookObject.length > 0) { %>
    <ul>
        <% for (let book in bookObject) { %>
            <li>
            <p>title = <%= bookObject[book].title %></p>
            <% if (bookObject[book].author != "") { %>
                <p>author = <%= bookObject[book].author %></p>
                <% } %>
            <p>summary = <%= bookObject[book].summary %></p>
            <% } %>
            </li>
        <% } %>
    </ul>
